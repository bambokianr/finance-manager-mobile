{
  "expo": {
    "name": "finance-manager-mobile",
    "slug": "finance-manager-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Front-end de aplicação mobile desenvolvida com React Native - matéria CES-26: Desenvolvimento de Aplicações para a Internet.",
    "githubUrl": "https://github.com/bambokianr/finance-manager-mobile"
  }
}
